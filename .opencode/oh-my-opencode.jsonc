{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "google_auth": false,
  "sisyphus_agent": {
    "disabled": false,
    "planner_enabled": true,
    "replace_plan": true,
    "default_builder_enabled": false
  },
  "claude_code": {
    "mcp": false,
    "commands": false,
    "skills": false,
    "agents": false,
    "hooks": false,
    "plugins": false
  },
  "agents": {
    "sisyphus": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    },
    "sisyphus-junior": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    },
    "oracle": {
      "model": "cpa-openai/gpt-5.2-codex",
      "variant": "high"
    },
    "librarian": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "explore": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "prometheus": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "max"
    },
    "metis": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "max"
    },
    "momus": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "max"
    },
    "atlas": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    }
  },
  "categories": {
    "visual-engineering": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    },
    "ultrabrain": {
      "model": "cpa-openai/gpt-5.2-codex",
      "variant": "max"
    },
    "artistry": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    },
    "quick": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "unspecified-low": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    },
    "unspecified-high": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "max"
    },
    "writing": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    }
  },
  "background_task": {
    "defaultConcurrency": 5
  },
  "experimental": {
    "preemptive_compaction_threshold": 0.8,
    "truncate_all_tool_outputs": true,
    "aggressive_truncation": true,
    "auto_resume": true
  }
}
